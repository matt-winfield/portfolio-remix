{
    "name": "portfolio-remix-5452",
    "private": true,
    "sideEffects": false,
    "type": "module",
    "imports": {
        "#*": "./*"
    },
    "scripts": {
        "build": "run-s build:*",
        "build:icons": "tsx ./other/build-icons.ts",
        "build:remix": "remix build --sourcemap",
        "build:server": "tsx ./other/build-server.ts",
        "predev": "pnpm run build:icons --silent",
        "dev": "remix dev -c \"node ./server/dev-server.js\" --manual",
        "prisma:studio": "prisma studio",
        "format": "prettier --write .",
        "lint": "eslint .",
        "generate": "pnpm rimraf --glob node_modules/.pnpm/**/node_modules/.prisma/ && prisma generate",
        "setup": "pnpm run build && pnpm run generate && prisma migrate dev && prisma db seed && playwright install",
        "start": "cross-env NODE_ENV=production node .",
        "start:mocks": "cross-env NODE_ENV=production MOCKS=true tsx .",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "test:e2e": "pnpm run test:e2e:dev",
        "test:e2e:dev": "playwright test --ui",
        "pretest:e2e:run": "pnpm run build",
        "test:e2e:run": "cross-env CI=true playwright test",
        "test:e2e:install": "npx playwright install --with-deps chromium",
        "typecheck": "tsc",
        "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
        "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\""
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "eslintIgnore": [
        "/node_modules",
        "/build",
        "/public/build",
        "/playwright-report",
        "/server-build"
    ],
